<div class="contribute subpage">
    <div class="wrapper">
        <div class="heading heading--big" data-animation="heading">
            <h2 class="heading__title js-title">{{title}}</h2>
            <p class="heading__subtitle js-subtitle">{{subtitle}}</p>
            <div class="heading__decors">
                <hr class="heading__hr js-heading-hr">
                <img src="./media/dots-dark.png" alt="" class="heading__dots">
                <hr class="heading__hr js-heading-hr">
            </div>
        </div>
        <section class="contribute__content">
            <aside class="filters filters--aside" data-component="ContributeFilters">
                <form class="form form--filters" action="" novalidate>
                    <div class="form__subcategory">
                        <button class="form__dropdown" aria-controls="con-sector">
                            <span>Sector</span>
                            <i class="icon-arr-down"></i>
                        </button>
                        <div id="con-sector" class="hidden" aria-hidden="true">
                            <div class="form__wrap">
                                <label class="form__row">
                                    <input type="checkbox" name="sector-all" id="f-sector-all" value="all" data-all-sectors>
                                    <span class="form__checkbox"></span>
                                    <span class="form__label">All</span>
                                </label>
                                <div class="form__row--sub">
                                    <div class="form__toggle">
                                        <input type="checkbox" name="agriculture-all" id="f-sector-agriculture-all" value="agriculture-all" data-all-subsectors>
                                        <span class="form__checkbox"></span>
                                        <span class="form__label">Agriculture</span>
                                        <i class="icon-plus" aria-controls="con-agriculture"></i>
                                    </div>
                                    <div id="con-agriculture" class="hidden">
                                        <div class="form__subdropdown">
                                            <label class="form__row">
                                                <input type="checkbox" name="agriculture-enteric-fermentation" id="f-agriculture-enteric-fermentation" value="enteric-fermentation">
                                                <span class="form__checkbox"></span>
                                                <span class="form__label">Enteric Fermentation</span>
                                            </label>
                                            <label class="form__row">
                                                <input type="checkbox" name="agriculture-rise-cultivation" id="f-agriculture-rise-cultivation" value="rise-cultivation">
                                                <span class="form__checkbox"></span>
                                                <span class="form__label">Rise Cultivation</span>
                                            </label>
                                            <label class="form__row">
                                                <input type="checkbox" name="agriculture-crop-residues" id="f-agriculture-crop-residues" value="crop-residues">
                                                <span class="form__checkbox"></span>
                                                <span class="form__label">Crop Residues</span>
                                            </label>
                                            <label class="form__row">
                                                <input type="checkbox" name="agriculture-cultivation-soils" id="f-agriculture-cultivation-soils" value="cultivation-of-organic-soils">
                                                <span class="form__checkbox"></span>
                                                <span class="form__label">Cultivation of organic soils</span>
                                            </label>
                                            <label class="form__row">
                                                <input type="checkbox" name="agriculture-synthetic-fertilizer" id="f-agriculture-synthetic-fertilizer" value="synthetic-fertilizer">
                                                <span class="form__checkbox"></span>
                                                <span class="form__label">Synthetic fertilizer</span>
                                            </label>
                                            <label class="form__row">
                                                <input type="checkbox" name="agriculture-manure-soils" id="f-agriculture-manure-soils" value="manure-applied-to-soils">
                                                <span class="form__checkbox"></span>
                                                <span class="form__label">Manure applied to soils</span>
                                            </label>
                                            <label class="form__row">
                                                <input type="checkbox" name="agriculture-manure-management" id="f-agriculture-manure-management" value="manure-management">
                                                <span class="form__checkbox"></span>
                                                <span class="form__label">Manure management</span>
                                            </label>
                                            <label class="form__row">
                                                <input type="checkbox" name="agriculture-pasture" id="f-agriculture-pasture" value="manure-left-on-pasture">
                                                <span class="form__checkbox"></span>
                                                <span class="form__label">Manure left on pasture</span>
                                            </label>
                                            <label class="form__row">
                                                <input type="checkbox" name="agriculture-burning" id="f-agriculture-burning" value="burning">
                                                <span class="form__checkbox"></span>
                                                <span class="form__label">Burning</span>
                                            </label>
                                        </div>
                                    </div>
                                </div>
                                <div class="form__row--sub">
                                    <div class="form__toggle">
                                        <input type="checkbox" name="forestry-all" id="f-sector-forestry-all" value="forestry-all" data-all-subsectors>
                                        <span class="form__checkbox"></span>
                                        <span class="form__label">forestry</span>
                                        <i class="icon-plus" aria-controls="con-forestry"></i>
                                    </div>
                                    <div id="con-forestry" class="hidden">
                                        <div class="form__subdropdown">
                                            <label class="form__row">
                                                <input type="checkbox" name="forestry-enteric-fermentation" id="f-forestry-enteric-fermentation" value="enteric-fermentation" data-subsector>
                                                <span class="form__checkbox"></span>
                                                <span class="form__label">Enteric Fermentation</span>
                                            </label>
                                            <label class="form__row">
                                                <input type="checkbox" name="forestry-rise-cultivation" id="f-forestry-rise-cultivation" value="rise-cultivation" data-subsector>
                                                <span class="form__checkbox"></span>
                                                <span class="form__label">Rise Cultivation</span>
                                            </label>
                                            <label class="form__row">
                                                <input type="checkbox" name="forestry-crop-residues" id="f-forestry-crop-residues" value="crop-residues" data-subsector>
                                                <span class="form__checkbox"></span>
                                                <span class="form__label">Crop Residues</span>
                                            </label>
