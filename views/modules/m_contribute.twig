                                                <input type="checkbox" name="forestry-cultivation-soils" id="f-forestry-cultivation-soils" value="cultivation-of-organic-soils" data-subsector>
                                                <span class="form__checkbox"></span>
                                                <span class="form__label">Cultivation of organic soils</span>
                                            </label>
                                            <label class="form__row">
                                                <input type="checkbox" name="forestry-synthetic-fertilizer" id="f-forestry-synthetic-fertilizer" value="synthetic-fertilizer" data-subsector>
                                                <span class="form__checkbox"></span>
                                                <span class="form__label">Synthetic fertilizer</span>
                                            </label>
                                            <label class="form__row">
                                                <input type="checkbox" name="forestry-manure-soils" id="f-forestry-manure-soils" value="manure-applied-to-soils" data-subsector>
                                                <span class="form__checkbox"></span>
                                                <span class="form__label">Manure applied to soils</span>
                                            </label>
                                            <label class="form__row">
                                                <input type="checkbox" name="forestry-manure-management" id="f-forestry-manure-management" value="manure-management" data-subsector>
                                                <span class="form__checkbox"></span>
                                                <span class="form__label">Manure management</span>
                                            </label>
                                            <label class="form__row">
                                                <input type="checkbox" name="forestry-pasture" id="f-forestry-pasture" value="manure-left-on-pasture" data-subsector>
                                                <span class="form__checkbox"></span>
                                                <span class="form__label">Manure left on pasture</span>
                                            </label>
                                            <label class="form__row">
                                                <input type="checkbox" name="forestry-burning" id="f-sector-burning" value="burning" data-subsector>
                                                <span class="form__checkbox"></span>
                                                <span class="form__label">Burning</span>
                                            </label>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="form__subcategory">
